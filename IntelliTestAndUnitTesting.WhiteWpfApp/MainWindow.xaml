<Window x:Class="IntelliTestAndUnitTesting.WhiteWpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IntelliTestAndUnitTesting.WhiteWpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="83,68,0,0" VerticalAlignment="Top" Width="120" AutomationProperties.AutomationId="MyList" SelectionChanged="comboBox_SelectionChanged">
            <ListBoxItem Content="One"/>
            <ListBoxItem Content="Two"/>
            <ListBoxItem Content="Three"/>

        </ComboBox>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="286,68,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" IsEnabled="False" AutomationProperties.AutomationId="MyButton"/>

    </Grid>
</Window>
